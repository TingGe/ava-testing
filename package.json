{
  "name": "ava-testing",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --inline --hot --content-base ./public",
    "test:watch": "tsc --watch | ava --watch",
    "pretest": "rm -rf reports && mkdirp reports/unit & tsc",
    "test": "npm run test:unit && npm run test:coverage && npm run test:uitest",
    "test:unit": "ava | tap-that-html > reports/unit/index.html",
    "test:coverage": "nyc --reporter=html --report-dir reports/coverage --reporter=text ava",
    "test:uitest": "source ./uitest.sh",
    "posttest": "rm -rf .nyc_output built coverage"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jsdom": "^11.4.0",
    "jsdom-global": "^3.0.2",
    "nyc": "^11.3.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.3.0",
    "sinon": "^4.1.2",
    "tap-that-html": "^1.0.1",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5"
  },
  "ava": {
    "require": [
      "./tests/helpers/setup.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false
  },
  "author": "TingGe<505253293@163.com>",
  "license": "MIT"
}
