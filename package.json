    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.3.0",
    "request": "^2.88.0",
    "rmfr": "^2.0.0",
    "swig": "^1.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "configs": {
    "server": "https://alibaba.github.io/defensor",
    "registry": "https://registry.npm.taobao.org",
    "cacheTTL": 3600000
  },
  "author": "TingGe<505253293@163.com>",
  "license": "MIT"
}
